{
  "name": "iot-widgets",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "description": "iot widgets",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./math": {
      "import": "./dist/math.js",
      "require": "./dist/math.cjs",
      "types": "./dist/math.d.ts"
    },
    "./event": {
      "import": "./dist/event.js",
      "require": "./dist/event.cjs",
      "types": "./dist/event.d.ts"
    },
    "./mui": {
      "import": "./dist/mui.js",
      "require": "./dist/mui.cjs",
      "types": "./dist/mui.d.ts"
    }
  },
  "files": [
    "/dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@mui/material": "^5.15.3",
    "preact": "^10.19.3",
    "stylis": "^4.3.1",
    "stylis-plugin-rtl": "^2.1.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@preact/preset-vite": "^2.7.0",
    "@types/uuid": "^9.0.7",
    "sass": "^1.69.7",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vite-plugin-dts": "^3.7.0",
    "vite-plugin-lib-inject-css": "^1.3.0"
  }
}
