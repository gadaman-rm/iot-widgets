{
  "name": "@gadaco/iot-widgets",
  "version": "0.7.2",
  "type": "module",
  "description": "iot widgets",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gadaman-rm/iot-widgets.git"
  },
  "license": "MIT",
  "keywords": [
    "web components"
  ],
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./components": {
      "import": "./dist/components.js",
      "require": "./dist/components.cjs",
      "types": "./dist/components.d.ts"
    },
    "./math": {
      "import": "./dist/math.js",
      "require": "./dist/math.cjs",
      "types": "./dist/math.d.ts"
    },
    "./event": {
      "import": "./dist/event.js",
      "require": "./dist/event.cjs",
      "types": "./dist/event.d.ts"
    },
    "./utility": {
      "import": "./dist/utility.js",
      "require": "./dist/utility.cjs",
      "types": "./dist/utility.d.ts"
    },
    "./styles/*": "./dist/styles/*",
    "./styles/adoptor/*": "./dist/styles/adoptor/*",
    "./styles/theme/*": "./dist/styles/theme/*"
  },
  "files": [
    "/dist"
  ],
  "sideEffects": [
    "**/*.scss"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "plop": "plop",
    "graph": "yarn --silent depcruise src --include-only \"^src\" --output-type dot > graph.dot"
  },
  "devDependencies": {
    "@types/uuid": "^9.0.8",
    "@z-code/vite-plugin-swc": "^0.3.3",
    "dependency-cruiser": "^16.3.1",
    "plop": "^4.0.1",
    "sass": "^1.77.1",
    "typescript": "^5.4.5",
    "vite": "^5.2.11",
    "vite-plugin-dts": "^3.9.1",
    "vite-plugin-lib-inject-css": "2.0.1"
  },
  "dependencies": {
    "@material/web": "^1.4.1",
    "js-base64": "^3.7.7",
    "material-components-web": "^14.0.0",
    "monaco-editor": "^0.49.0",
    "uuid": "^9.0.1"
  },
  "packageManager": "yarn@4.2.2+sha512.c44e283c54e02de9d1da8687025b030078c1b9648d2895a65aab8e64225bfb7becba87e1809fc0b4b6778bbd47a1e2ab6ac647de4c5e383a53a7c17db6c3ff4b"
}
