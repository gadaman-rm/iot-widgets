{
  "name": "@gadaman-rm/iot-widgets",
  "version": "0.1.2",
  "type": "module",
  "description": "iot widgets",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./components": {
      "import": "./dist/components.js",
      "require": "./dist/components.cjs",
      "types": "./dist/components.d.ts"
    },
    "./math": {
      "import": "./dist/math.js",
      "require": "./dist/math.cjs",
      "types": "./dist/math.d.ts"
    },
    "./event": {
      "import": "./dist/event.js",
      "require": "./dist/event.cjs",
      "types": "./dist/event.d.ts"
    },
    "./utility": {
      "import": "./dist/utility.js",
      "require": "./dist/utility.cjs",
      "types": "./dist/utility.d.ts"
    }
  },
  "files": [
    "/dist"
  ],
  "publishConfig": {
    "@gadaman-rm:registry": "https://npm.pkg.github.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gadaman-rm/iot-widgets.git"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview"
  },
  "devDependencies": {
    "@types/uuid": "^9.0.7",
    "sass": "^1.70.0",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vite-plugin-dts": "^3.7.2",
    "vite-plugin-lib-inject-css": "^1.3.0"
  },
  "dependencies": {
    "@material/web": "^1.2.0",
    "eventemitter3": "^5.0.1",
    "material-components-web": "^14.0.0",
    "uuid": "^9.0.1"
  }
}
